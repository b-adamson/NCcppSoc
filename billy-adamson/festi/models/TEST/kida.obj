# Reference to the MTL file
mtllib kida.mtl

# Vertices
v -1.0 0.0 -1.0
v -1.0 0.0 1.0
v 1.0 0.0 1.0
v 1.0 0.0 -1.0

# Texture Coordinates
vt 0.0 0.0
vt 0.0 1.0
vt 1.0 1.0
vt 1.0 0.0

# Normals
vn 0.0000 1.0000 0.0000

# Apply the material to the entire object
usemtl kida

# Faces
f 1/1/1 3/3/1 2/2/1
f 3/3/1 1/1/1 4/4/1
